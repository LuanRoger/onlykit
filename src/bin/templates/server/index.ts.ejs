import { Hono } from "onlykit/server";
import { serve } from "onlykit/server/node";

const app = new Hono();

app.get("/", (c) => {
  return c.text("Hello World");
});

export function main() {
  serve(app, (info) => {
    console.log(`Server is running on http://localhost:${info.port}`);
  });
}

main();
